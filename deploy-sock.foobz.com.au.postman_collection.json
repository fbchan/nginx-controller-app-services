					"raw": "{\n  \"metadata\": {\n    \"name\": \"sock.foobz.com.au\",\n    \"displayName\": \"Sock Shop\",\n    \"tags\": []\n  },\n  \"desiredState\": {\n    \"ingress\": {\n      \"gatewayRefs\": [\n        {\n          \"ref\": \"/services/environments/t2-production/gateways/tenant2-nginx-plus-inst1\"\n        },\n        {\n          \"ref\": \"/services/environments/t2-production/gateways/tenant2-nginx-plus-inst2\"\n        }\n      ],\n      \"uris\": {\n        \"https://{{FQDN}}\": {}\n      },\n      \"tls\": {\n        \"certRef\": {\n          \"ref\": \"/services/environments/t2-production/certs/foobz.com.au\"\n        },\n        \"protocols\": [\n          \"TLSv1.3\",\n          \"TLSv1.2\"\n        ],\n        \"preferServerCipher\": \"DISABLED\",\n        \"sessionCache\": \"OFF\"\n      }\n    },\n    \"backend\": {\n      \"workloadGroups\": {\n        \"sockshop-backend-server\": {\n          \"loadBalancingMethod\": {\n            \"type\": \"ROUND_ROBIN\"\n          },\n          \"uris\": {\n            \"{{BACKEND_01}}\": {\n              \"isBackup\": false,\n              \"isDown\": false,\n              \"isDrain\": false,\n              \"resolve\": \"DISABLED\"\n            }\n          }\n        }\n      },\n      \"monitoring\": {\n        \"defaultState\": \"HEALTHY\",\n        \"uri\": \"/\"\n      }\n    },\n    \"logging\": {\n      \"errorLog\": \"ENABLED\",\n      \"accessLog\": {\n        \"state\": \"ENABLED\"\n      }\n    }\n  }\n}",
